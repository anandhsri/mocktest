{
  "family": "neet-mock-test",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::868522398039:role/ecsTaskExecutionRole",
  "taskRoleArn": "arn:aws:iam::868522398039:role/ecsTaskRole",
  "containerDefinitions": [
    {
      "name": "neet-mock-test-container",
      "image": "YOUR_ECR_REPO_URI:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8040,
          "protocol": "tcp"
        }
      ],
      "environment": [
        {
          "name": "PORT",
          "value": "8040"
        },
        {
          "name": "HOST",
          "value": "0.0.0.0"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/ecs/neet-mock-test",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
        "healthCheck": {
          "command": [
            "CMD-SHELL",
            "python3 -c \"import urllib.request; urllib.request.urlopen('http://localhost:8040/')\" || exit 1"
          ],
        "interval": 30,
        "timeout": 3,
        "retries": 3,
        "startPeriod": 60
      }
    }
  ]
}
